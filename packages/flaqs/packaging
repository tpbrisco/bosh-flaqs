# abort script on any command that exits with a non zero value
set -ev
echo CWD is `pwd`
echo installing flaqs.tar into ${BOSH_INSTALL_TARGET}
ls
D=`pwd`
cd ${BOSH_INSTALL_TARGET}
tar xvf ${D}/flaqs.tar
